{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "rules": {
    "no-unused-vars": "off"
  },
  "eslint.alwaysShowStatus": false,
  "eslint.notifyOnRuleChange": "off",
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.run": "manual"
}
